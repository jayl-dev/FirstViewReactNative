{
  "expo": {
    "name": "MyFirstView",
    "slug": "MyFirstView",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "jay.liang",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jlsoft.MyFirstView",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "androidStatusBar": {
        "translucent": false,
        "backgroundColor": "#ffffff",
        "barStyle": "dark-content"
      },
      "androidNavigationBar": {
        "visible": "leanback",
        "barStyle": "dark-content",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jlsoft.MyFirstView"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-maps",
        {
          "iosGoogleMapsApiKey": "TODO",
          "androidGoogleMapsApiKey": "APIKEY"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ID"
      },
      "GEOCODING_API_KEY": "APIKEY"
    }
  }
}
